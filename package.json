{
  "name": "my-turborepo",
  "version": "0.1.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "storybook": "turbo run storybook --filter=@ui/library",
    "test": "turbo run test --filter=@ui/library",
    "build-storybook": "turbo run build-storybook --filter=@ui/library",
    "check-types": "turbo run check-types",
    "view-report": "turbo run view-report"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "@edge-runtime/vm": "latest",
    "@storybook/react": "latest",
    "@storybook/react-vite": "latest",
    "@storybook/test": "latest",
    "@testing-library/dom": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@types/debug": "latest",
    "@vitejs/plugin-react": "latest",
    "@vitest/browser": "latest",
    "@vitest/ui": "latest",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "4.1.0",
    "happy-dom": "latest",
    "jiti": "latest",
    "jsdom": "latest",
    "lightningcss": "latest",
    "lucide-react": "^0.454.0",
    "next": "15.2.4",
    "path": "latest",
    "react": "^19",
    "react-dom": "^19",
    "storybook": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.6",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitest": "latest"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8",
    "tailwindcss": "^3.4.17",
    "turbo": "^2.5.0",
    "typescript": "^5"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
